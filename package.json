{
  "name": "cio-js-snippet-playground",
  "version": "1.0.0",
  "description": "A place to test the CIO JS Web SDK",
  "main": ".eleventy.js",
  "scripts": {
    "start": "rm -rf _site && npx @11ty/eleventy --serve --quiet & npx tailwindcss -i ./src/tailwind.css -o ./_site/css/style.css --watch",
    "build": "rm -rf _site && ELEVENTY_ENV=production npx @11ty/eleventy && npx tailwindcss -i ./src/tailwind.css -o ./_site/css/style.css --minify"
  },
  "keywords": [],
  "author": "jordanhailey",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "tailwindcss": "^3.3.1"
  }
}
